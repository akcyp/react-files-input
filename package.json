{
  "name": "react-files-uploader",
  "description": "React component to upload multiple files",
  "version": "0.0.1",
  "author": "akcyp",
  "license": "ISC",
  "keywords": [
    "react",
    "files",
    "upload"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akcyp/react-files-uploader.git"
  },
  "homepage": "https://github.com/akcyp/react-files-uploader#readme",
  "bugs": {
    "url": "https://github.com/akcyp/react-files-uploader/issues"
  },
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
