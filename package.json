    "test": "vitest",
    "preview": "vite preview",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix",
    "*.{js,jsx,ts,tsx,md,html,css}": "prettier --write"
